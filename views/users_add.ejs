<%- include header %>
<%- include navbar %>		
<div class="container">
        <form class="form-horizontal" role="form" method="post">
          <div class="form-group">
              <div class="input-group input-group-lg  col-xs-4 col-xs-offset-4">
                    <span class="input-group-addon">编号</span>
                    <input type="text" class="form-control" id="id" name="id" placeholder="请输入编号" required>
              </div>
          </div>
          <div class="form-group">
              <div class="input-group input-group-lg  col-xs-4 col-xs-offset-4">
                    <span class="input-group-addon">体重</span>
                    <input type="text" class="form-control" id="weight" name="weight" placeholder="请输入体重" required>
              </div>
          </div>     
          <div class="form-group">
              <div class="input-group input-group-lg  col-xs-4 col-xs-offset-4">
                    <span class="input-group-addon">年龄</span>
                    <input type="text" class="form-control" id="age" name="age" placeholder="请输入年龄" required>
              </div>
          </div> 
          <div class="form-group">
            <div class="input-group input-group-lg col-xs-4 col-xs-offset-4">
                    <span class="input-group-addon">进区时间</span>
                    <input type="text" class="form-control" id="date" name="date" placeholder="请输入进区时间">
            </div>
          </div>      

          <div class="form-group">
            <div class="col-xs-offset-5">
              <button type="submit" class="btn btn-info  btn-lg">提交</button>
              <button type="reset" class="btn btn-warning  btn-lg">重置</button>
            </div>
          </div>
        </form>
    </div>
<%- include footer %>